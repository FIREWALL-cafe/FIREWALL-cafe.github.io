{
  "framework": "create-react-app",
  "buildCommand": "yarn build",
  "outputDirectory": "build",
  "devCommand": "yarn start:client",
  "installCommand": "yarn install",
  "regions": [
    "iad1"
  ],
  "functions": {
    "api/proxy.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/searches/:id/images",
      "destination": "/api/search-images"
    },
    {
      "source": "/searches",
      "destination": "/api/searches"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/proxy"
    },
    {
      "source": "/proxy-image",
      "destination": "/api/proxy-image"
    }
  ],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}